// 2013 phones in literal notation

var phone1 = {
	"make": "Samsung",
	"model": "Galaxy S4",
	"version": "Kit Kat 4.4",
	"update": function(){
		if(phone1.version === "Kit Kat 4.4"){
			alert("Your phone will recieve an update to Android L");
		}
	}
};

var tableRow = Ti.UI.createTableViewRow({
	title: phone1.make,	
	desc: phone1.model,
	hasDetail: true
});

phones2013.add(tableRow);
tableRow.addEventListener("click", getDetail);

phones.setData(phone2013);

